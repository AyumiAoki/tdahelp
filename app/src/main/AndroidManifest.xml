<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tdahelp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen"></activity>
        <activity android:name=".TelaJogarSemCadastro" />
        <activity android:name=".TelaMenu" />
        <activity
            android:name=".TelaColorir"
            android:theme="@style/AppTheme.Colorir" />
        <activity
            android:name=".TelaQuebraCabeca"
            android:theme="@style/AppTheme.QuebraCabeca" />
        <activity
            android:name=".TelaMemoria"
            android:theme="@style/AppTheme.Memory" />
        <activity android:name=".TelaCadastro" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>